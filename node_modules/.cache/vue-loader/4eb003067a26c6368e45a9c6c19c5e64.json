      showSnackbar: false,\n    })\n  },\n  methods: {\n    scrapeResults() {\n      const queryOptions = {\n        query: this.query,\n        location: this.location,\n        jobType: this.jobType,\n        maxAge: this.maxAge,\n        sort: 'date',\n        limit: '0',\n      };\n\n      this.jsonData = null;\n      this.scraping = true;\n\n      Indeed.query(queryOptions).then(res => {\n        this.scraping = false;\n        this.showSnackbar = true;\n        this.saveResults(res); // An array of Job objects\n      });\n    },\n    saveResults(res) {\n      this.jsonData = res;\n      // eslint-disable-next-line\n      console.log(this.jsonData, 'Done');\n    },\n    viewResults() {\n      // Todo: Add display for results\n    },\n  },\n}\n</script>\n\n<style scoped>\n.md-card {\n  margin: auto;\n  padding-bottom: 20px;\n}\n\n.md-card-actions {\n  display: inline;\n}\n</style>\n"]}]}